#include <iostream>
#include <string>

using namespace std;

int main() {
    //set the three names
    string name1;
    string name2;
    string name3;
    //Ask the three names
    cout<< "Enter the first name: "<<endl;
    getline(cin, name1);
    cout<< "Enter the second name: "<<endl;
    getline(cin, name2);
    cout<< "Enter the third name: "<<endl;
    getline(cin, name3);
    //Print the frist name1
    cout<< "The first name you entered is: "<< name1<<endl;
    
    return 0;
}
